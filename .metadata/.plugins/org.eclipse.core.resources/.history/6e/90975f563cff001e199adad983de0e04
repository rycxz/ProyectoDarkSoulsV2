package objetos;

import jefes.Jefes;

public class Armas extends Objetos {
	
	private String tipoArama;
	private int danio;
	private String tipoDanio;
	
	public Armas(String nombre, String lore, double durabilidad, Jefes perteneciaJefe,String tipoArama,int danio,String tipoDanio) {
		super(nombre, lore, durabilidad, perteneciaJefe);
		// TODO Auto-generated constructor stub
		this.tipoArama=tipoArama;
		this.danio=danio;
		this.tipoDanio=tipoDanio;
	}

	

	
	
	

}
